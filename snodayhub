nd return nil end local function l() local bpko = workspace.CurrentCamera local nbpkor = bpko.CFrame.Position local mnbpk = bpko.CFrame.LookVector * 700  local emnbpk = RaycastParams.new() emnbpk.FilterType = Enum.RaycastFilterType.Exclude  emnbpk.FilterDescendantsInstances = {game.Players.LocalPlayer.Character} local lemnbp = workspace:Raycast(nbpkor, mnbpk, emnbpk) if lemnbp then local igqdjch = lemnbp.Instance local hslemnb = p(igqdjch) if hslemnb then print("\72\105\116\32\112\108\97\121\101\114\58", hslemnb.Name) return hslemnb else print("\72\105\116\32\111\98\106\101\99\116\44\32\98\117\116\32\110\111\32\112\108\97\121\101\114\32\100\101\116\101\99\116\101\100\46") end else print("\78\111\32\104\105\116\46") end return nil end local function mn(tigqdjc) if tigqdjc and tigqdjc.Character and tigqdjc.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local bpkor = game.Players.LocalPlayer local jchs = bpkor.Character.HumanoidRootPart.Position local djchs = tigqdjc.Character.HumanoidRootPart.Position local qdjch = (djchs - jchs).Unit local gqdjch = qdjch * _G.ClickForce gqd(gqdjch,tigqdjc.Character,true) end end rf_tig.Button1Down:Connect(function() if _G.Click == true then  local igqdjch = rf_tig.Target local tigqdjc = l() if tigqdjc and tigqdjc ~= game.Players.LocalPlayer then mn(tigqdjc) end end end) local function c(hslemnb, rf_tigq, orf_tigq) if hslemnb and hslemnb.Character and hslemnb.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local _tigqdj = hslemnb.Character.HumanoidRootPart.Position return (rf_tigq - _tigqdj).Magnitude <= orf_tigq end return false end local function g() local bpkor = game.Players.LocalPlayer if bpkor and bpkor.Character and bpkor.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local rf_tigq = bpkor.Character.HumanoidRootPart.Position local orf_tigq = _G.Distance local kor = game.Players:GetPlayers() fo