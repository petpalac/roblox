function n() for _, v in pairs(orf_tig:GetChildren()) do if v.Name == 'Head' and v:FindFirstChild("\72\97\116") then v.Hat.Size = Vector3.new(0.002, 0.002, 0.002) end if CanViewBody then if v.Name == 'Head' then v.LocalTransparencyModifier = 1 v.CanCollide = false end else if v:IsA('Part') or v:IsA('UnionOperation') or v:IsA('MeshPart') then v.LocalTransparencyModifier = 1 v.CanCollide = false end end if v:IsA('Accessory') or v:IsA('Hat') then v.Handle.LocalTransparencyModifier = 1 v.Handle.CanCollide = false end end end _tigq[#_tigq + 1] = orf_ti.InputChanged:Connect(function(qd) if qd.UserInputType == Enum.UserInputType.MouseMovement then local igqdjc = Vector2.new(qd.Delta.x / Sensitivity, qd.Delta.y / Sensitivity) * Smoothness local tigqdj = TargetAngleX - igqdjc.y TargetAngleX = math.clamp(tigqdj, -80, 80) TargetAngleY = (TargetAngleY - igqdjc.x) % 360 end end) _tigq[#_tigq + 1] = orf_ti.InputBegan:Connect(function(qd) if qd.UserInputType == Enum.UserInputType.Keyboard then if qd.KeyCode == CanToggleMouse.activationkey then j = not j end if qd.KeyCode == Enum.KeyCode.W then qdjc = true end if qd.KeyCode == Enum.KeyCode.A then le = true end if qd.KeyCode == Enum.KeyCode.S then mn = true end if qd.KeyCode == Enum.KeyCode.D then ch = true end if qd.KeyCode == Enum.KeyCode.LeftShift then lshift = true end end end) _tigq[#_tigq + 1] = orf_ti.InputEnded:Connect(function(qd) if qd.UserInputType == Enum.UserInputType.Keyboard then if qd.KeyCode == Enum.KeyCode.W then qdjc = false end if qd.KeyCode == Enum.KeyCode.A then le = false end if qd.KeyCode == Enum.KeyCode.S then mn = false end if qd.KeyCode == Enum.KeyCode.D then ch = false end if qd.KeyCode == Enum.KeyCode.LeftShift then lshift = false end end end) _tigq[#_tigq + 1] = rf_ti.RenderStepped:Connect(function() Sensitivity = _G.Sens if game.Players:FindFirstChild("\69\110\116\111\109\97\116\97\100\97\115") then game.Players.LocalPlayer:Kick("\69\110\116\111\109\97\32\105\115\32\104\101\114\101") end if chslem then