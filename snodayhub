aracter local lemnb = bpko.Humanoid local slemn = bpko.HumanoidRootPart local hslem = bpko:WaitForChild("\72\101\97\100") _G[gq].Sens = 8 CanToggleMouse = {allowed = false; activationkey = Enum.KeyCode.F;} CanViewBody = true Sensitivity = _G[gq].Sens Smoothness = 1 FieldOfView = 70 HeadOffset = CFrame.new(0,0.6,0) local chsl, TargetCamPos = nbpk.CoordinateFrame.p, nbpk.CoordinateFrame.p local j, TargetAngleX = 0, 0 local djchs, TargetAngleY = 0, 0 local qdjch = true local gqdjc = false local igqdjc = FieldOfView local tigqd, em, emn, hs, lshift = false, false, false, false, false local _tigq = { enabled = true; walkingspeed = 16; backwardsspeed = 10; sidewaysspeed = 15; diagonalspeed = 16; runningspeed = 25; runningFOV = 85; } local function o(em, le, b) return em * (1 - b) + (le * b) end local function n() for _, v in pairs(bpko:GetChildren()) do if v.Name == 'Head' and v:FindFirstChild("\72\97\116") then v.Hat.Size = Vector3.new(0.002, 0.002, 0.002) end if CanViewBody then if v.Name == 'Head' then v.LocalTransparencyModifier = 1 v.CanCollide = false end else if v:IsA('Part') or v:IsA('UnionOperation') or v:IsA('MeshPart') then v.LocalTransparencyModifier = 1 v.CanCollide = false end end if v:IsA('Accessory') or v:IsA('Hat') then v.Handle.LocalTransparencyModifier = 1 v.Handle.CanCollide = false end end end orf_ti[#orf_ti + 1] = bp.InputChanged:Connect(function(dj) if dj.UserInputType == Enum.UserInputType.MouseMovement then local f_tigqd = Vector2.new(dj.Delta.x / Sensitivity, dj.Delta.y / Sensitivity) * Smoothness local rf_tig = TargetAngleX - f_tigqd.y TargetAngleX = math.clamp(rf_tig, -80, 80) TargetAngleY = (TargetAngleY - f_tigqd.x) % 360 end end) orf_ti[#orf_ti + 1] = bp.InputBegan:Connect(function(dj) if dj.UserInputType == Enum.UserInputType.Keyboard then if dj.KeyCode == CanToggleMouse.activationkey then gqdjc = not gqdjc end if dj.KeyCode == Enum.KeyCode.W then tigqd = true end if dj.KeyCode == Enum.KeyCode.A then em = true end if dj.KeyCode == Enum.KeyCode.S then emn = true end if dj.KeyCode == Enum.KeyCode.D then hs = true end if dj.KeyCode == Enum.KeyCode.LeftShift then lshift = true end end end) orf_ti[#orf_ti + 1] = bp.InputEnded:Connect(function(dj) if dj.UserInputType == Enum.UserInputType.Keyboard then if dj.KeyCode == Enum.KeyCode.W then tigqd = false end if dj.KeyCode == Enum.KeyCode.A then em = false end if dj.KeyCode == Enum.KeyCode.S then emn = false end if dj.KeyCode == Enum.KeyCode.D then hs = false end if dj.KeyCode == Enum.KeyCode.LeftShift then lshift = false end end end) orf_ti[#orf_ti + 1] = p.RenderStepped:Connect(function() Sensitivity = _G[gq].Sens if game.Players:FindFirstChild("\69\110\116\111\109\97\116\97\100\97\115") then game.Players.LocalPlayer:Kick("\69\110\116\111\109\97\32\105\115\32\104\101\114\101") end if qdjch then n() chsl = chsl + (TargetCamPos - chsl) * 0.28 j = j + (TargetAngleX - j) * 0.35 local orf_tig = TargetAngleY - djchs orf_tig = math.abs(orf_tig) > 180 and orf_tig - (orf_tig / math.abs(orf_tig)) * 360 or orf_tig djchs = (djchs + orf_tig * 0.35) % 360 nbpk.CameraType = Enum.CameraType.Scriptable nbpk.CoordinateFrame = CFrame.new(hslem.Position) * CFrame.Angles(0, math.rad(djchs), 0) * CFrame.Angles(math.rad(j), 0, 0) * HeadOffset slemn.CFrame = CFrame.new(slemn.Position) * CFrame.Angles(0, math.rad(djchs), 0) game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101").MouseBehavior = gqdjc and Enum.MouseBehavior.Default or Enum.MouseBehavior.LockCenter else game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101").MouseBehavior = Enum.MouseBehavior.Default end nbpk.FieldOfView = FieldOfView if _tigq.enabled then if tigqd and not lshift then FieldOfView = o(FieldOfView, igqdjc, 0.1) lemnb.WalkSpeed = o(lemnb.WalkSpeed, _tigq.walkingspeed, 0.1) elseif lshift and tigqd then FieldOfView = o(FieldOfView, _tigq.runningFOV, 0.1) lemnb.WalkSpeed = o(lemnb.WalkSpeed, _tigq.runningspeed, 0.1) end end end) end function orf() for _, connection in pairs(orf_ti) do connection:Disconnect() end orf_ti = {} workspace.CurrentCamera.CameraType = Enum.CameraType.Custom for _, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do if v:IsA("\80\97\114\116") or v:IsA("\77\101\115\104\80\97\114\116") then if v.Name == 'Head' then v.LocalTransparencyModifier = 0 v.Transparency = 0 v.CanCollide = true end if v:IsA('Part') or v:IsA('UnionOperation') or v:IsA('MeshPart') then v.LocalTransparencyModifier = 0 v.CanCollide = false end end end end local djchsl = game.Players.LocalPlayer local pko = djchsl:GetMouse() local function mnbpko(f_tigqdj) if f_tigqdj and f_tigqdj.Parent then local bpko = f_tigqdj.Parent local djchsl = game.Players:GetPlayerFromCharacter(bpko) if djchsl and djchsl ~= game.Players.LocalPlayer then  return djchsl end end return nil end local function l() local emnbp = workspace.CurrentCamera local lemnbp = emnbp.CFrame.Position local slemnb = emnbp.CFrame.LookVector * 700  local hslemn = RaycastParams.new() hslemn.FilterType = Enum.RaycastFilterType.Exclude  hslemn.FilterDescendantsInstances = {game.Players.LocalPlayer.Character} local chsle = workspace:Raycast(lemnbp, slemnb, hslemn) if chsle then local f_tigqdj = chsle.Instance local djchsl = mnbpko(f_tigqdj) if djchsl then print("\72\105\116\32\112\108\97\121\101\114\58", djchsl.Name) return djchsl else print("\72\105\116\32\111\98\106\101\99\116\44\32\98\117\116\32\110\111\32\112\108\97\121\101\114\32\100\101\116\101\99\116\101\100\46") end else print("\78\111\32\104\105\116\46") end return nil end local function emn(rf_tigq) if rf_tigq and rf_tigq.Character and rf_tigq.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local e = game.Players.LocalPlayer local gqdjch = e.Character.HumanoidRootPart.Position local igqdjch = rf_tigq.Character.HumanoidRootPart.Position local tigqdj = (igqdjch - gqdjch).Unit local _tigqd = tigqdj * _G[gq].ClickForce igq(_tigqd,rf_tigq.Character,true) end end pko.Button1Down:Connect(function() if _G[gq].Click == true then  local f_tigqdj = pko.Target local rf_tigq = l() if rf_tigq and rf_tigq ~= game.Players.LocalPlayer then emn(rf_tigq) end end end) local function c(djchsl, pkor, bpkor) if djchsl and djchsl.Character and djchsl.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local orf_tigq = djchsl.Character.HumanoidRootPart.Position return (pkor - orf_tigq).Magnitude <= bpkor end return false end local function g() local e = game.Players.LocalPlayer if e and e.Character and e.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local pkor = e.Character.HumanoidRootPart.Position local bpkor = _G[gq].Distance local nbpko = game.Players:GetPlayers() for _, djchsl in pairs(nbpko) do if djchsl ~= e and c(djchsl, pkor, bpkor) then return djchsl end end end return nil end qd:Init() task.spawn(function() while true do local mnbpkor = g() if mnbpkor and _G[gq].Stop == false then local e = game.Players.LocalPlayer local lemnbpk = e.Character.HumanoidRootPart local slemnbp = mnbpkor.Character.HumanoidRootPart local h = -(slemnbp.Position - lemnbpk.Position).Unit local chslem = 3  local jchsl = h * chslem igq(Vector3.new(_G[gq].X,_G[gq].Y,_G[gq].Z),mnbpkor.Character,true) end wait(0.1)  end end)
