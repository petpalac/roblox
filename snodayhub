_G.ClickForce = mn end     }) _G.Distance = 20 _G.Stop = true _G.X = 1 _G.Y = 50 _G.Z = 1 _G.Anti = false _G.Click = false _G.ClickForce = 50 game.ReplicatedStorage.Remotes.Combat.Hit.OnClientEvent:Connect(function(em,le,sl,hs) if _G.Anti then task.wait() game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.zero end end) local tigqd = {} function rf() local slemnbp = game:GetService("\80\108\97\121\101\114\115").LocalPlayer repeat wait() until slemnbp.Character local f_tig = game:GetService("\82\117\110\83\101\114\118\105\99\101") local rf_ti = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") local orf_t = game.Workspace.CurrentCamera local k = slemnbp:GetMouse() local rf_tig = slemnbp.Character local bpkor = rf_tig.Humanoid local nbpk = rf_tig.HumanoidRootPart local mnbpk = rf_tig:WaitForChild("\72\101\97\100") _G.Sens = 8 CanToggleMouse = {allowed = false; activationkey = Enum.KeyCode.F;} CanViewBody = true Sensitivity = _G.Sens Smoothness = 1 FieldOfView = 70 HeadOffset = CFrame.new(0,0.6,0) local emnbp, TargetCamPos = orf_t.CoordinateFrame.p, orf_t.CoordinateFrame.p local lemnb, TargetAngleX = 0, 0 local slemnb, TargetAngleY = 0, 0 local hslemn = true local chsl = false local j = FieldOfView local djch, em, nb, hs, lshift = false, false, false, false, false local qdjch = { enabled = true; walkingspeed = 16; backwardsspeed = 10; sidewaysspeed = 15; diagonalspeed = 16; runningspeed = 25; runningFOV = 85; } local function o(em, le, b) return em * (1 - b) + (le * b) end local function n() for _, v in pairs(rf_tig:GetChildren()) do if v.Name == 'Head' and v:FindFirstChild("\72\97\116") then v.Hat.Size = Vector3.new(0.002, 0.002, 0.002) end if CanViewBody then if v.Name == 'Head' then v.LocalTransparencyModifier = 1 v.CanCollide = false end else if v:IsA('Part') or v:IsA('UnionOperation') or v:IsA('MeshPart') then v.LocalTransparencyModifier = 1 v.CanCollide = false end end if v:IsA('Accessory') or v:IsA('Hat') then v.Handle.LocalTransparencyModifier = 1 v.Handle.CanCollide = false end end end tigqd[#tigqd + 1] = rf_ti.InputChanged:Connect(function(dj) if dj.UserInputType == Enum.UserInputType.MouseMovement then local gqdjch = Vector2.new(dj.Delta.x / Sensitivity, dj.Delta.y / Sensitivity) * Smoothness local igqdjc = TargetAngleX - gqdjch.y TargetAngleX = math.clamp(igqdjc, -80, 80) TargetAngleY = (TargetAngleY - gqdjch.x) % 360 end end) tigqd[#tigqd + 1] = rf_ti.InputBegan:Connect(function(dj) if dj.UserInputType == Enum.UserInputType.Keyboard then if dj.KeyCode == CanToggleMouse.activationkey then chsl = not chsl end if dj.KeyCode == Enum.KeyCode.W then djch = true end if dj.KeyCode == Enum.KeyCode.A then em = true end if dj.KeyCode == Enum.KeyCode.S then nb = true end if dj.KeyCode == Enum.KeyCode.D then hs = true end if dj.KeyCode == Enum.KeyCode.LeftShift then lshift = true end end end) tigqd[#tigqd + 1] = rf_ti.InputEnded:Connect(function(dj) if dj.UserInputType == Enum.UserInputType.Keyboard then if dj.KeyCode == Enum.KeyCode.W then djch = false end if dj.KeyCode == Enum.KeyCode.A then em = false end if dj.KeyCode == Enum.KeyCode.S then nb = false end if dj.KeyCode == Enum.KeyCode.D then hs = false end if dj.KeyCode == Enum.KeyCode.LeftShift then lshift = false end end end) tigqd[#tigqd + 1] = f_tig.RenderStepped:Connect(function() Sensitivity = _G.Sens if game.Players:FindFirstChild("\69\110\116\111\109\97\116\97\100\97\115") then game.Players.LocalPlayer:Kick("\69\110\116\111\109\97\32\105\115\32\104\101\114\101") end if hslemn then n() emnbp = emnbp + (TargetCamPos - emnbp) * 0.28 lemnb = lemnb + (TargetAngleX - lemnb) * 0.35 local tigqdj = TargetAngleY - slemnb tigqdj = math.abs(tigqdj) > 180 and tigqdj - (tigqdj / math.abs(tigqdj)) * 360 or tigqdj slemnb = (slemnb + tigqdj * 0.35) % 360 orf_t.CameraType = Enum.CameraType.Scriptable orf_t.CoordinateFrame = CFrame.new(mnbpk.Position) * CFrame.Angles(0, math.rad(slemnb), 0) * CFrame.Angles(math.rad(lemnb), 0, 0) * HeadOffset nbpk.CFrame = CFrame.new(nbpk.Position) * CFrame.Angles(0, math.rad(slemnb), 0) game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101").MouseBehavior = chsl and Enum.MouseBehavior.Default or Enum.MouseBehavior.LockCenter else game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101").MouseBehavior = Enum.MouseBehavior.Default end orf_t.FieldOfView = FieldOfView if qdjch.enabled then if djch and not lshift then FieldOfView = o(FieldOfView, j, 0.1) bpkor.WalkSpeed = o(bpkor.WalkSpeed, qdjch.walkingspeed, 0.1) elseif lshift and djch then FieldOfView = o(FieldOfView, qdjch.runningFOV, 0.1) bpkor.WalkSpeed = o(bpkor.WalkSpeed, qdjch.runningspeed, 0.1) end end end) end function orf() for _, connection in pairs(tigqd) do connection:Disconnect() end tigqd = {} workspace.CurrentCamera.CameraType = Enum.CameraType.Custom for _, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do if v:IsA("\80\97\114\116") or v:IsA("\77\101\115\104\80\97\114\116") then if v.Name == 'Head' then v.LocalTransparencyModifier = 0 v.Transparency = 0 v.CanCollide = true end if v:IsA('Part') or v:IsA('UnionOperation') or v:IsA('MeshPart') then v.LocalTransparencyModifier = 0 v.CanCollide = false end end end end local slemnbp = game.Players.LocalPlayer local f_tigq = slemnbp:GetMouse() local function k(gqdjchs) if gqdjchs and gqdjchs.Parent then local rf_tig = gqdjchs.Parent local slemnbp = game.Players:GetPlayerFromCharacter(rf_tig) if slemnbp and slemnbp ~= game.Players.LocalPlayer then  return slemnbp end end return nil end local function l() local pko = workspace.CurrentCamera local bpkorf = pko.CFrame.Position local nbpko = pko.CFrame.LookVector * 700  local mnbpko = RaycastParams.new() mnbpko.FilterType = Enum.RaycastFilterType.Exclude  mnbpko.FilterDescendantsInstances = {game.Players.LocalPlayer.Character} local emnbpk = workspace:Raycast(bpkorf, nbpko, mnbpko) if emnbpk then local gqdjchs = emnbpk.Instance local slemnbp = k(gqdjchs) if slemnbp then print("\72\105\116\32\112\108\97\121\101\114\58", slemnbp.Name) return slemnbp else print("\72\105\116\32\111\98\106\101\99\116\44\32\98\117\116\32\110\111\32\112\108\97\121\101\114\32\100\101\116\101\99\116\101\100\46") end else print("\78\111\32\104\105\116\46") end return nil end local function nb(igqdjch) if igqdjch and igqdjch.Character and igqdjch.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local pkor = game.Players.LocalPlayer local chsle = pkor.Character.HumanoidRootPart.Position local jchs = igqdjch.Character.HumanoidRootPart.Position local djchs = (jchs - chsle).Unit local qdjchs = djchs * _G.ClickForce qdj(qdjchs,igqdjch.Character,true) end end f_tigq.Button1Down:Connect(function() if _G.Click == true then  local gqdjchs = f_tigq.Target local igqdjch = l() if igqdjch and igqdjch ~= game.Players.LocalPlayer then nb(igqdjch) end end end) local function c(slemnbp, f_tigqd, rf_tigq) if slemnbp and slemnbp.Character and slemnbp.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local tigqdjc = slemnbp.Character.HumanoidRootPart.Position return (f_tigqd - tigqdjc).Magnitude <= rf_tigq end return false end local function g() local pkor = game.Players.LocalPlayer if pkor and pkor.Character and pkor.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local f_tigqd = pkor.Character.HumanoidRootPart.Position local rf_tigq = _G.Distance local orf_ti = game.Players:GetPlayers() for _, slemnbp in pairs(orf_ti) do if slemnbp ~= pkor and c(slemnbp, f_tigqd, rf_tigq) then return slemnbp end end end return nil end qd:Init() task.spawn(function() while true do local kor = g() if kor and _G.Stop == false then local pkor = game.Players.LocalPlayer local bpkorf_ = pkor.Character.HumanoidRootPart local nbpkor = kor.Character.HumanoidRootPart local mnbpkor = -(nbpkor.Position - bpkorf_.Position).Unit local e = 3  local lemnbp = mnbpkor * e qdj(Vector3.new(_G.X,_G.Y,_G.Z),kor.Character,true) end wait(0.1)  end end)
